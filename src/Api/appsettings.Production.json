{
    "Serilog": {
        "MinimumLevel": {
            "Override": {
                "Api.Features.TelegramTest.Services.UpdateHandler.UpdateHandlerService": "Debug"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs/api-logs-.json",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 5,
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
                }
            }
        ]
    },
    "ConnectionStrings": {
        "Telegram": ""
    },
    "App": {
        "BaseUrl": ""
    }
}