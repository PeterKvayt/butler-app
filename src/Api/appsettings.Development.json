{
    "Serilog": {
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "logs/api-logs-.json",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 5,
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
                }
            }
        ]
    },
    "ConnectionStrings": {
        "Telegram": "{telegram connection string}"
    },
    "App": {
        "BaseUrl": "https://localhost:7124"
    }
}