{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/api-logs-.json",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 5,
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "App": {
    "BaseUrl": "https://pointedly-conquering-retriever.cloudpub.ru:443"
  },
  "Telegram": {
    "WebhookSecret": "{value_from_dev_secrets}",
    "BotToken": "{secret_value}"
  }
}